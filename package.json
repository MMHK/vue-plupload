{
    "name": "vue-plupload",
    "version": "2.1.2",
    "description": "plupload wrapper for vue",
    "main": "index.js",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --mode production --target lib --name VuePlupload index.js",
        "lint": "vue-cli-service lint",
        "publish": "npm config set registry https://registry.npmjs.com && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mm-sam/vue-plupload.git"
    },
    "keywords": [
        "vue",
        "plupload"
    ],
    "author": "Sam",
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/mm-sam/vue-plupload/issues"
    },
    "homepage": "https://github.com/mm-sam/vue-plupload#readme",
    "dependencies": {
        "plupload": "^2.3.2",
        "vue": "^2.3.4"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.7.0",
        "@vue/cli-plugin-babel": "^3.4.1",
        "@vue/cli-plugin-eslint": "^3.4.1",
        "@vue/cli-service": "^3.4.1",
        "babel-eslint": "^10.0.1",
        "bootstrap": "3",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "^4.9.0",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.5.21"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 0.12%"
    ]
}
